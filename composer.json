{
  "name": "philippgrashoff/settingsforatk",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.3"
    }
  },
  "require": {
    "atk4/data": "2.3.*",
    "atk4/core": "2.3.*",
    "philippgrashoff/traitsforatkdata": "master",
    "philippgrashoff/mtomforatk": "master",
    "ext-sodium": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">8"
  },
  "autoload": {
    "psr-4": {
      "usermessageforatk\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "usermessageforatk\\tests\\": "tests"
    }
  }
}
